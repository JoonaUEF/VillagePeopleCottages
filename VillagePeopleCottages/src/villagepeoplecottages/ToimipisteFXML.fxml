<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="mainPane" maxHeight="300.0" maxWidth="250.0" minHeight="250.0" minWidth="250.0" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="villagepeoplecottages.ToimipisteFXMLController">
   <children>
      <GridPane AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints fillHeight="false" minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Nimi" />
            <Label text="Lähiosoite" GridPane.rowIndex="1" />
            <Label text="Postinumero" GridPane.rowIndex="2" />
            <Label text="Postitoimipaikka" GridPane.rowIndex="3" />
            <Label text="Puhelinnumero" GridPane.rowIndex="4" />
            <Label text="Email" GridPane.rowIndex="5" />
            <TextField fx:id="nimiTextField" GridPane.columnIndex="1" />
            <TextField fx:id="lahiosoiteTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="postinumeroTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="postitoimipaikkaTextField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="puhelinnumeroTextField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="emailTextField" GridPane.columnIndex="1" GridPane.rowIndex="5" />
         </children>
      </GridPane>
      <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="50.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <items>
            <Button fx:id="peruutaButton" layoutX="10.0" layoutY="18.0" mnemonicParsing="false" onAction="#SuljeIkkuna" prefWidth="75.0" text="Peruuta" />
          <Button fx:id="LisaaButton" mnemonicParsing="false" onAction="#LisaaUusiToimipiste" prefWidth="75.0" text="Lisää" />
        </items>
         <padding>
            <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
         </padding>
      </ToolBar>
   </children>
</AnchorPane>
